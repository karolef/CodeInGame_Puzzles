# WORK IN PROGRESS (last condition fails probably because of spaces)

import sys
import math

message = input()

bin_message = ''.join(format(ord(letter), 'b') for letter in message)

if len(bin_message) < 7:
    bin_message = '0' + bin_message
#print(bin_message)

is_zero = '00 '
is_one = '0 '
more = '0'

temp_char = ''
final_char = ''
count = 0


for char in bin_message:
    if char is temp_char:
        count +=1
    else:
        if temp_char is '0':
            final_char += is_zero
        elif temp_char is '1':
            final_char += is_one
        for i in range(0, count):
            final_char += more
        if count > 0:
            final_char += ' '
            
        temp_char = char
        count = 1
        
if temp_char is '0':
    final_char += is_zero
elif temp_char is '1':
    final_char += is_one
for j in range(0, count):
    final_char += more

print (final_char)
